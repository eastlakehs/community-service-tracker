{
  "name": "@ehs-service/firebase-firestore",
  "type": "module",
  "version": "0.0.22",
  "description": "Firestore configs and authentication tests",
  "scripts": {
    "compile": "tsc",
    "test": "npm run test:full",
    "deploy": "npm run deploy:firestore",
    "test:only": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --forceExit",
    "test:full": "npm run compile && firebase emulators:exec --only firestore \"npm run test:only\"",
    "deploy:firestore": "`npm bin`/firebase deploy --only firestore"
  },
  "author": "Daniel Sudzilouski <daniel@sudzilouski.com>",
  "homepage": "https://github.com/eastlakehs/community-service-tracker",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eastlakehs/community-service-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/eastlakehs/community-service-tracker/issues"
  },
  "engines": {
    "node": "16"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@firebase/testing": "^0.20.11",
    "@types/jest": "^28.1.4",
    "@types/node": "^14.6.4",
    "babel-jest": "^26.3.0",
    "firebase-tools": "^9.8.0",
    "jest": "^28.1.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "firebase": "^7.19.1"
  }
}
